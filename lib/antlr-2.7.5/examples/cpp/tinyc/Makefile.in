	$(objdir)/TinyCLexerTokenTypes.txt \
	$(objdir)/TinyCTokenTypes.txt \
	$(eol)

lexer_g_gen_FILES = \
	$(lexer_g_cxx_FILES) \
	$(lexer_g_hpp_FILES) \
	$(lexer_g_txt_FILES) \
	$(eol)

lexer_g_obj_FILES = \
	$(objdir)/TinyCLexer@OBJEXT@ \
	$(eol)

tinyc_g_FILES = \
	$(srcdir)/tinyc.g \
	$(eol)

